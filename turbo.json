{
  "pipeline": {
    "start": {
      "dependsOn": ["^start"],
      "outputs": ["dist/**", ".next/**"]
    },
    "app:dev": {
      "dependsOn": ["^app:dev"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:watch": {
      "dependsOn": ["^build:watch"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**"]
    },
    "serve": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  }
}
